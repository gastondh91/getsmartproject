{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": ["error", "never"],
        "quotes": ["error", "single"],
        "react/prop-types": [0],
        "react/destructuring-assignment" : [0],
        "no-const-assign": [0],
        "max-len": [0],
        "react/no-array-index-key": [0],
        "no-use-before-define": [0],
        "no-console": [0],
        "arrow-parens": [0],
    }
}
